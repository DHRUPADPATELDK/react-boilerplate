{
  "name": "react-pwa-boilerplate",
  "version": "1.0.0",
  "description": "React PWA Boilerplate",
  "scripts": {
    "start": "pawjs --no-cache --env=development start",
    "build:prod": "node --max_old_space_size=4096  ./node_modules/.bin//../@pawjs/pawjs/paw.js --no-cache --env-config-path=./.env --env=production build && npm run gulp",
    "start:prod": "npm run build:prod && npm run serve:prod",
    "serve:prod": "node ./dist/server.js",
    "check:update": "ncu -u",
    "lint": "eslint --ext .tsx,.js src/ --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy:run": "cypress run --record --key xxxx-xxx-xxxx --group front-tests --ci-build-id",
    "cy:open": "./node_modules/.bin/cypress open",
    "gulp": "gulp"
  },
  "lint-staged": {
    "*.{js,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "React",
    "SSR",
    "PWA"
  ],
  "author": "Cloud Monastery",
  "license": "ISC",
  "homepage": "https://bitbucket.org/cloudmonastery/reactpwa-boilerplate#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@loadable/component": "^5.12.0",
    "@pawjs/image-optimizer": "2.2.6",
    "@pawjs/pawjs": "^3.0.2-beta.5",
    "@pawjs/redux": "2.3.0",
    "@pawjs/sass": "2.2.5",
    "@pawjs/srcset": "2.1.6",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "iframe-resizer": "^4.2.10",
    "luxon": "^1.23.0",
    "memory-cache": "^0.2.0",
    "node-cache": "^5.1.0",
    "react-cookie": "^4.0.3",
    "react-currency-format": "^1.0.0",
    "react-lazy-progressive-image": "^1.5.5",
    "react-lazyload": "^2.6.7",
    "react-localize-redux": "^3.5.3",
    "react-meta-tags": "^0.7.4",
    "react-modal": "^3.11.2",
    "react-paginate": "^6.3.2",
    "react-progressive-image": "^0.6.0",
    "react-rating": "^2.0.5",
    "react-redux": "^7.2.0",
    "react-responsive-picture": "^3.2.2",
    "react-scroll": "^1.7.16",
    "react-sticky-el": "^2.0.4",
    "react-structured-data": "0.0.14",
    "react-swipeable": "^5.5.1",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.5",
    "redux-saga": "^1.1.3",
    "ua-parser-js": "^0.7.21",
    "universal-cookie": "^4.0.3",
    "universal-cookie-express": "^4.0.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/luxon": "^1.22.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-modal": "^3.10.5",
    "@types/react-paginate": "^6.2.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-scroll": "^1.5.4",
    "@types/react-sticky-el": "^1.0.2",
    "@types/redux-form": "^8.2.3",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "create-react-component-folder": "^0.2.0",
    "cypress": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-gzip": "^1.4.2",
    "gulp-strip-comments": "^2.5.2",
    "gulp-uglify-es": "^2.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
